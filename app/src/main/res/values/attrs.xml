<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LongTextView">
        <attr name="longText" format="string"/>
    </declare-styleable>

    <declare-styleable name="MultiLineSingleCheckbox">
        <attr name="itemOneLine" format="integer"/>
    </declare-styleable>

    <declare-styleable name="HintView">
        <attr name="showIcon" format="boolean"/>
        <attr name="iconText" format="string"/>
        <attr name="iconMargin" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TimePicker">
        <attr name="timeStyle" format="integer">
            <enum name="simple" value="0"/>
            <enum name="full" value="1"/>
        </attr>
    </declare-styleable>
</resources>